{
    "C_Cpp.errorSquiggles": "enabled",
    "editor.fontFamily": "\"Terminess Nerd Font\", \"SanariFontB002\"",
    "files.associations": {
        "*.html": "html",
        "*.py": "python",
        "*.css": "css",
        "*.js": "javascript",
        "tuple": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "__hash_table": "cpp",
        "__tree": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "*.tcc": "cpp"
    },
    "terminal.integrated.fontFamily": "\"Terminus\"",
    "editor.fontSize": 15.5,
    
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
          "<C-n>": false,
          "<C-p>": false
        }
}