{
    "C_Cpp.errorSquiggles": "enabled",
    "editor.fontFamily": "Terminess Nerd Font",
    "files.associations": {
        "*.py": "python",
        "*.html": "html",
        "*.css": "css",
        "*.js": "javascript",
        "tuple": "cpp",
        "cmath": "cpp",
        "complex": "cpp"
    },
    "terminal.integrated.fontFamily": "\"Terminus\"",
    "editor.fontSize": 15.5,
    
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
          "<C-n>": false,
          "<C-p>": false
        }
}